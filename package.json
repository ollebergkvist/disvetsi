{
  "author": "Olle Bergkvist",
  "license": "MIT",
  "name": "disvetsi",
  "private": true,
  "version": "0.1.0",
  "engines": {
    "npm": "please-use-yarn",
    "yarn": "1.x"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "yarn build && yarn node seo-analyzer.js"
    }
  },
  "scripts": {
    "barrel:generate": "./bash/generate_component.sh",
    "build": "tinacms build && next build",
    "circular": "npx madge  --ts-config ./tsconfig.json --extensions ts,tsx --circular ./src",
    "clean": "rimraf node_modules",
    "component:generate": "./bash/generate_component.sh",
    "dev": "tinacms dev -c \"next dev\"",
    "eslint:format": "eslint --write \"src/**/*.ts\"",
    "eslint:lint": "eslint ./src --ext .ts/.tsx",
    "lint": "next lint",
    "predev": "vercel env pull && graphql-codegen -w",
    "prepare": "husky install",
    "prettier:check": "yarn prettier --check .",
    "prettier:write": "yarn prettier --write .",
    "seo:analyze": "NODE_ENV=development yarn build && yarn node seo-analyzer.js",
    "start": "tinacms build && next start",
    "storybook:build": "storybook build",
    "storybook": "storybook dev -p 6006",
    "test": "jest --coverage"
  },
  "dependencies": {
    "@tinacms/auth": "^1.0.3",
    "@tinacms/cli": "^1.5.22",
    "@types/node": "20.3.3",
    "@types/react": "18.2.14",
    "@types/react-dom": "18.2.6",
    "autoprefixer": "10.4.14",
    "class-variance-authority": "^0.6.1",
    "dayjs": "^1.11.9",
    "dotenv": "^16.3.1",
    "eslint": "8.44.0",
    "eslint-config-next": "13.4.7",
    "next": "13.4.7",
    "postcss": "8.4.24",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-hook-form": "^7.45.1",
    "tinacms": "^1.5.13",
    "zustand": "^4.3.8"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.6.6",
    "@commitlint/config-conventional": "^17.6.6",
    "@graphql-codegen/typescript-graphql-request": "^5.0.0",
    "@storybook/addon-essentials": "^7.0.24",
    "@storybook/addon-interactions": "^7.0.24",
    "@storybook/addon-links": "^7.0.24",
    "@storybook/addon-styling": "^1.3.2",
    "@storybook/blocks": "^7.0.24",
    "@storybook/nextjs": "^7.0.24",
    "@storybook/react": "^7.0.24",
    "@storybook/testing-library": "^0.0.14-next.2",
    "@tailwindcss/aspect-ratio": "^0.4.2",
    "@tailwindcss/forms": "^0.5.3",
    "@tailwindcss/typography": "^0.5.9",
    "@testing-library/react": "^14.0.0",
    "eslint-plugin-storybook": "^0.6.12",
    "graphql-codegen": "^0.4.0",
    "graphql-request": "^6.1.0",
    "husky": "^8.0.3",
    "jest": "^29.5.0",
    "lint-staged": "^13.2.3",
    "next-sitemap": "^4.1.8",
    "rimraf": "^5.0.1",
    "storybook": "^7.0.24",
    "tailwindcss": "^3.3.2",
    "typescript": "^5.1.6"
  }
}
